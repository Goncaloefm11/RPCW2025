<http://www.semanticweb.org/gugafm11/ontologies/2026/cinema> rdf:type owl:Ontology ;
                                                              rdfs:comment "Uma ontologia sobre cinema"@pt .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Atuou
:Atuou rdf:type owl:ObjectProperty ;
       owl:inverseOf :temAtor ;
       rdf:type owl:FunctionalProperty ;
       rdfs:domain :Pessoa ;
       rdfs:range :Filme .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#compôs
:compôs rdf:type owl:ObjectProperty ;
        owl:inverseOf :foiComposto ;
        rdfs:domain :Pessoa ;
        rdfs:range :Obra .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#escreveu
:escreveu rdf:type owl:ObjectProperty ;
          rdfs:domain :Pessoa ;
          rdfs:range :Obra .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#foiComposto
:foiComposto rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#foiEscrito
:foiEscrito rdf:type owl:ObjectProperty ;
            owl:inverseOf :foiEscrito .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#foiRealizado
:foiRealizado rdf:type owl:ObjectProperty ;
              owl:inverseOf :realizou .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#realizou
:realizou rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Pessoa ;
          rdfs:range :Filme .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#representa
:representa rdf:type owl:ObjectProperty ;
            rdfs:domain :Ator ;
            rdfs:range :Personagem .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temArgumento
:temArgumento rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temAtor
:temAtor rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temGénero
:temGénero rdf:type owl:ObjectProperty ;
           rdfs:domain :Filme ;
           rdfs:range :Género .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temLingua
:temLingua rdf:type owl:ObjectProperty ;
           rdfs:domain :Pais .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temPaisOrigem
:temPaisOrigem rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temPersonagem
:temPersonagem rdf:type owl:ObjectProperty ;
               owl:inverseOf :éPersonagem ;
               rdfs:domain :Filme ;
               rdfs:range :Personagem .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temPeçaMusical
:temPeçaMusical rdf:type owl:ObjectProperty ;
                rdfs:domain :Filme ;
                rdfs:range :PeçaMusical .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#éPersonagem
:éPersonagem rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#data
:data rdf:type owl:DatatypeProperty ;
      rdfs:range xsd:string .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#duracao
:duracao rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:integer .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#temSexo
:temSexo rdf:type owl:DatatypeProperty ;
         rdfs:domain :Pessoa ;
         rdfs:range xsd:string .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#titulo
:titulo rdf:type owl:DatatypeProperty ;
        rdfs:domain :Filme ;
        rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Argumento
:Argumento rdf:type owl:Class ;
           rdfs:subClassOf :Obra .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Ator
:Ator rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Pessoa
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :Atuou ;
                                                   owl:someValuesFrom :Filme
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:subClassOf :Pessoa ,
                      [ rdf:type owl:Class ;
                        owl:unionOf ( :AtorFeminina
                                      :AtorMasculino
                                    )
                      ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#AtorFeminina
:AtorFeminina rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :temSexo ;
                                    owl:hasValue "F"
                                  ] ;
              rdfs:subClassOf :Ator .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#AtorMasculino
:AtorMasculino rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :temSexo ;
                                     owl:hasValue "M"
                                   ] ;
               rdfs:subClassOf :Ator .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Escritor
:Escritor rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Pessoa
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :escreveu ;
                                                       owl:someValuesFrom [ rdf:type owl:Class ;
                                                                            owl:unionOf ( :Argumento
                                                                                          :Livro
                                                                                        )
                                                                          ]
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Pessoa .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Filme
:Filme rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :data ;
                         owl:someValuesFrom xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :duracao ;
                         owl:someValuesFrom xsd:integer
                       ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#FilmesAventura
:FilmesAventura rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Filme
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :temGénero ;
                                                             owl:hasValue :Aventura
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#FilmesDramáticos
:FilmesDramáticos rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Filme
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :temGénero ;
                                                               owl:hasValue :Drama
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#FilmesInfantis
:FilmesInfantis rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Filme
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :temGénero ;
                                                             owl:hasValue :Infantil
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#FilmesInteressantes
:FilmesInteressantes rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :Filme
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :temGénero ;
                                                                  owl:minCardinality "2"^^xsd:nonNegativeInteger
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#FilmesRomanticos
:FilmesRomanticos rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Filme
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :temGénero ;
                                                               owl:hasValue :Romance
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#FilmesTerror
:FilmesTerror rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( :Filme
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :temGénero ;
                                                           owl:hasValue :Terror
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Género
:Género rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :Acao
                                          :Aventura
                                          :Comédia
                                          :Drama
                                          :Ficcao
                                          :Infantil
                                          :Romance
                                          :Terror
                                          :Thriller
                                        )
                            ] .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Livro
:Livro rdf:type owl:Class ;
       rdfs:subClassOf :Obra .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Músico
:Músico rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Pessoa
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :compôs ;
                                                     owl:someValuesFrom :PeçaMusical
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Pessoa .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Obra
:Obra rdf:type owl:Class .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Pais
:Pais rdf:type owl:Class .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Personagem
:Personagem rdf:type owl:Class .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Pessoa
:Pessoa rdf:type owl:Class .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#PeçaMusical
:PeçaMusical rdf:type owl:Class ;
             rdfs:subClassOf :Obra .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#Realizador
:Realizador rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :realizou ;
                                  owl:someValuesFrom :Filme
                                ] ;
            rdfs:subClassOf :Pessoa .


###  http://www.semanticweb.org/gugafm11/ontologies/2026/cinema#longasMetragens
:longasMetragens rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( :Filme
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty :duracao ;
                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                   owl:onDatatype xsd:integer ;
                                                                                   owl:withRestrictions ( [ xsd:minExclusive 60
                                                                                                          ]
                                                                                                        )
                                                                                 ]
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] .


